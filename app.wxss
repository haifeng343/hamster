/**app.wxss**/
page {
  background-color: #F2F2F2;
  border-top: 2rpx solid #F2F2F2;
  box-sizing: border-box;
}

.container {
  width: 94.67%;
  height: auto;
  margin: 0 auto;
  border-radius: 30rpx;
  background-color: #fff;
}

.right {
  width: 10rpx;
  height: 18rpx;
}

.gou {
  width: 32rpx;
  height: 20rpx;
}

/* 新增地址底部按钮 */
.address_btn {
  width:622rpx;
  height:80rpx;
  background: linear-gradient(90deg, rgba(98, 179, 255, 1) 0%, rgba(79, 119, 255, 1) 100%);
  border-radius: 42rpx;
  position: fixed;
  bottom: 52rpx;
  left: 50%;
  font-size: 36rpx;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  transform: translateX(-50%);
  z-index: 1;
}

/* 新增地址兼容iPhoneX */
.address_btn_phx {
  bottom: 80rpx !important;
}

.Fixed28 {
  bottom: 68rpx !important;

}

.button-hover {
  background-color: transparent;
}

/* 没有数据展示 */
.no-show {
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #BCBCBC;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
}

.noImg1 {
  width: 286rpx;
  height: 212rpx;
  margin-bottom: 50rpx;
}

.noImg2 {
  width: 330rpx;
  height: 232rpx;
  margin-bottom: 50rpx;
}

/* 地址列表和新增地址 */
.list-address {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.item-address {
  width: 100%;
  height: auto;
  min-height: 152rpx;
  box-sizing: border-box;
  align-items: center;
  border-bottom: 4rpx solid #F2F2F2;
  border-top: 4rpx solid #F2F2F2;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  position: relative;
  margin-bottom: -4rpx;
}

.item-address:first-child {
  border-top: 0;
}

.item-address-radio {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  padding-left: 40rpx;
  box-sizing: border-box;
}

.item-address-right {
  display: flex;
  width: 500rpx;
  overflow: hidden;
  box-sizing: border-box;
  padding-left: 40rpx;
  flex-direction: column;
  justify-content: space-between;
  font-size: 24rpx;
  color: #7C7C7D;
  padding: 40rpx 0;
  margin-left: 50rpx;
}

.item-addres-top {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  align-items: center;
}

.item-address-name {
  width: 140rpx;
  display: flex;
  overflow: hidden;
  font-size: 28rpx;
  color: #3A3A3A;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-address-delect {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  right: 40rpx;
  top: 40rpx;
}


radio .wx-radio-input {
  width: 36rpx;
  height: 36rpx;
  position: relative;
  box-sizing: border-box;
}


/* 选中后的 背景样式 */
radio .wx-radio-input.wx-radio-input-checked {
  transform: translate(-50%, -50%) scale();
  border-radius: 50%;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  border: none !important;
  width: 36rpx;
  height: 36rpx;
}

/* 选中后的 对勾样式  */
radio .wx-radio-input.wx-radio-input-checked::before {
  /* 去除对号 */
  content: '';
}


checkbox .wx-checkbox-input {
  width: 36rpx;
  height: 36rpx;
  position: relative;
  border-radius: 100%;
  border: 1rpx solid #BCBCBC;
  box-sizing: border-box;
}


/* 选中后的 背景样式 */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  transform: translate(-50%, -50%) scale();
  border-radius: 50%;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  border: none !important;
  width: 36rpx;
  height: 36rpx;
}

/* 选中后的 对勾样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  /* 去除对号 */
  content: '';
}

input {
  z-index: 1;
}

/* 客服按钮 */
.contact {
  width: 408rpx!important;
  height: 100rpx!important;
  background: rgba(241, 241, 241, 1);
  border-radius: 50rpx;
  margin: 84rpx auto 160rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #B9B9B9;
}

.contact-img {
  width: 47rpx;
  height: 47rpx;
  margin-right: 18rpx;
}

.wrap {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break: keep-all;
}

.flex-end {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

.more {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 40rpx;
}

.columnWrap {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}

/* 解决图片底部像素差 */
img {
  float: left;
  font-size: 0;
  object-fit: cover;
  vertical-align: bottom;
}

button {
  background: transparent;
  padding: 0 !important;
  margin: 0 !important;
  font-weight: normal !important;
}

.b68 {
  height: 168rpx !important;
}

/* 遮罩层 */
.mask {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1;
}

.shadow {
  box-shadow: 0 4rpx 16rpx rgba(0, 0, 0, 0.3) !important;
}

.switch {
  zoom: 0.8;
}

/* 客服悬浮 */
.contactFix {
  width: 128rpx !important;
  height: 82rpx !important;
  position: fixed !important;
  z-index: 6;
  bottom: 120rpx;
  right: 0;
  margin: 0 !important;
  padding: 0 !important;
  border-radius: 0 !important;
}

.contactFixImg {
  width: 128rpx;
  height: 82rpx;
}