page{
  background: #fff;
}
.login{
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}
.img1{
  width: 710rpx;
  height: 452rpx;
  border-radius: 16rpx;
  margin: 26rpx auto 30rpx;
}
.btnTxt{
  font-size: 40rpx;
  width: 100%;
  height: auto;
  display: flex;
  margin: 100rpx auto;
  align-items: center;
  justify-content: center;
}
.input1{
  width: 69.33%;
  height: 76rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  border-radius: 40rpx;
  background: #EBEAEA;
  position: relative;
  margin-top: 50rpx;
  background-image: url('data:image/png;base64,');
  background-size: 40rpx 40rpx;
  background-repeat: no-repeat;
  background-position: 42rpx center;
}
.input2{
  background-image: url('data:image/png;base64,');
}
.placeholder{
  color: #D3D4D5;
}
.input1::after{
  content: '';
  position: absolute;
  width: 2rpx;
  height: 34rpx;
  background: #979797;
  top: 50%;
  left: 96rpx;
  transform: translateY(-50%);
}
.code{
  position: absolute;
  right: 36rpx;
  top: 50%;
  color: #333;
  transform: translateY(-50%);
  background: transparent!important;
  display: inline-block;
  font-size: 20rpx;
}
.gray{
  color: gray;
  opacity: 0.5;
}
.now{
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  font-size: 26rpx;
  color: #333;
  align-items: center;
  margin: 32rpx auto 0;
}
.address_btn{
  position: relative;
  left: 0;
  transform: translateX(0);
  bottom: 0;
  margin-top: 100rpx;
}

checkbox .wx-checkbox-input {
  width: 18rpx;
  height: 18rpx;
}


/* 选中后的 背景样式 */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  
  width: 18rpx;
  height: 18rpx;
}

/* 选中后的 对勾样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  /* 去除对号 */
  content: '';
}