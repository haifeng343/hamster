page {
  background-color: #fff;
  border-top: none;
}

.container1 {
  width: 94.67%;
  height: auto;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  margin: 0 auto;
}

.content {
  width: 100%;
  height: 550rpx;
  margin: 18rpx auto;
  position: relative;
}

.swiperContainer {
  width: 100%;
  height: 510rpx;
  position: relative;
}

.swiperContainer swiper-item {
  width: 100%;
  height: 480rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.swiperContainer .wx-swiper-dot {
  display: inline-flex;
  justify-content: space-between;
  border-radius: 50%;
  height: 10rpx;
  width: 10rpx;
}

.swiperContainer .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  border-radius: 50%;
}

.swiperContainer .wx-swiper-dot-active {
  width: 29rpx;
  height: 10rpx;
  border: none;
  border-radius: 4rpx;
  background: #327DFB;
}

.swiperContainer .wx-swiper-dot-active::before {
  border-radius: 50%;
}

.img1 {
  width: 90%;
  height: 446rpx;
  border-radius: 16rpx;
}

.tips {
  width: 380rpx;
  height: 50rpx;
  background: linear-gradient(180deg, rgba(100, 185, 255, 1) 0%, rgba(78, 114, 255, 1) 100%);
  border-radius: 26rpx;
  position: absolute;
  right: 0;
  top: 12rpx;
  font-size: 20rpx;
  color: #fff;
  box-sizing: border-box;
  padding-left: 14rpx;
  display: flex;
  align-items: center;
}

.close {
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  right: 16rpx;
  top: 50%;
  transform: translateY(-50%);
}

.hand {
  width: 28rpx;
  height: 32rpx;
  position: absolute;
  right: 15%;
  top: -20rpx;
}

.input1 {
  width: 85%;
  height: 72rpx;
  background: #81AFFC;
  border-radius: 36rpx;
  font-size: 26rpx;
  margin: 0 auto 0;
  box-sizing: border-box;
  padding-left: 72rpx;
  background-image: url('data:image/png;base64,');
  background-size: 42rpx 42rpx;
  background-repeat: no-repeat;
  background-position: 18rpx center;
}

.inputPlaceholder {
  color: #FFFFFF;
}

.ul {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  padding: 0 56rpx;
  margin-top: 215rpx;
  box-sizing: border-box;
}

.li {
  width: 198rpx;
  height: 204rpx;
  display: flex;
  margin-right: 22rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #7C7C7D;
  box-sizing: border-box;
  box-shadow: 0 0 30rpx 0 rgba(222, 229, 242, 1);
  border-radius: 22rpx;
  margin-bottom: 48rpx;
}

.li:active{
  box-shadow: 14px 14px 20px rgba(0, 0, 0, 0.1),
  -14px -14px 20px rgba(255, 255, 255, 1),
  inset 14px 14px 20px rgba(0, 0, 0, 0.1),
  inset -14px -14px 20px rgba(255, 255, 255, 1);
}
.li:nth-child(3n) {
  margin-right: 0;
}

.liImg {
  width: 77rpx;
  height: 77rpx;
  margin-bottom: 18rpx;
}

.share {
  width: 100%;
  height: 320rpx;
  display: flex;
  flex-direction: column;
  margin: 14rpx auto 46rpx;
  background-color: #f2f2f2;
}

.indexTop {
  width: 100%;
  height: 390rpx;
  background-color: #327DFB;
  border-radius: 0 0 50% 50%;
  padding-top: 26rpx;
}

.share-top {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: flex-start;
  box-sizing: border-box;
  padding-left: 126rpx;
  position: relative;
  color: #7C7C7D;
  font-size: 24rpx;
}

.bell {
  position: absolute;
  left: 56rpx;
  top: 50%;
  width: 50rpx;
  height: 50rpx;
  transform: translateY(-50%);
}

.msg {
  position: absolute;
  left: 86rpx;
  top: 8rpx;
  width: 24rpx;
  height: 20rpx;
}

.shareImg {
  width: 100%;
  height: 240rpx;
  background: rgba(199, 212, 249, 1);
  border-radius: 0 0 16rpx 16rpx;
}