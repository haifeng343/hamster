page{
  background-color: #fff;
}
.container1{
  width: 94.67%;
  height: auto;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
}
.searchTop1{
  width: 100%;
  height: 112rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0rpx auto;
  position: relative;
  z-index: 1;
  background: #fff;
}
.clear{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 20%;
  z-index: 5;
}
.input1{
  width: 600rpx;
  height:72rpx;
  background:rgba(242,242,242,1)!important;
  border-radius:36rpx;
  box-sizing: border-box;
  padding-left: 72rpx;
  padding-right: 100rpx;
  font-size: 26rpx;
  background-image: url('data:image/png;base64,')!important;
  background-size: 42rpx 42rpx!important;
  background-repeat: no-repeat!important;
  background-position: 14rpx center!important;
}
.searchTxt{
  width: 130rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  color: #4A90E2;
}
.banner{
  width: 100%;
  height: 176rpx;
  border-radius: 8rpx;
}
.list{
  margin-top: 0!important;
}
.listitem{
  padding:0!important;
  display: flex;
  width: 100%;
  margin: 20rpx auto 0;
}
.img1{
  width: 248rpx!important;
  height: 248rpx!important;
  margin-right: 20rpx!important;
  border-radius: 12rpx;
}
.item-title {
  width: 442rpx!important;
}
.end{
  width: 62rpx;
  height: 42rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  margin: 44rpx auto;
  color: #E1E1E1;
}
.end::after{
  content: '';
  position: absolute;
  width: 228rpx;
  height: 2rpx;
  background: #E1E1E1;
  left: 102rpx;
  top: 50%;
  transform: translateY(-50%);
}
.end::before{
  content: '';
  position: absolute;
  width: 228rpx;
  height: 2rpx;
  background: #E1E1E1;
  right: 102rpx;
  top: 50%;
  transform: translateY(-50%);
}
.contact{
  background: #f2f2f2;
  margin: 0 auto!important;
}
.noHasContainer{
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  margin: 78rpx auto 28rpx;
  font-size: 28rpx;
  color: #BCBCBC;
}

@import '../../pages/list/list.wxss'