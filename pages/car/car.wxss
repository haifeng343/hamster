/* 底部固定购买 */
.footer-fixed {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0 20rpx;
  background-color: #fff;
  z-index: 2;
  position: fixed;
  bottom: 0;
  left: 0;
}

.allcheck {
  font-size: 24rpx;
  color: #BCBCBC;
  margin-right: 22rpx;
}

.footer-fixed-left {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  align-items: center;
}

.footer-fiexd-content {
  width: 400rpx;
  height: auto;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}

.footer-fixed-txt1 {
  font-size: 30rpx;
  color: #000;
}
.p1{
  color: #F06902;
  font-size: 32rpx;
  font-weight:500;
}
.footer-fixed-txt2 {
  font-size: 26rpx;
  color: #2A6DFF;
}

.footer-fixed-small {
  font-size: 20rpx;
}

.footer-fixed-btn {
  width:382rpx;
height:74rpx;
background:linear-gradient(90deg,rgba(84,166,255,1) 0%,rgba(42,109,255,1) 100%);
  border-radius: 38rpx;
  color: #fff;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  letter-spacing: 4rpx;
}

.mChange {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin: 20rpx 0;
  align-items: center;
}

.tmChangeTxt {
  margin-right: 20rpx;
}

.content {
  width: 94.67%;
  height: auto;
  background: rgba(255, 255, 255, 1);
  border-radius: 30rpx;
  margin: 0 auto 20rpx;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  justify-content: flex-start;
}

.content:first-child {
  margin-top: 20rpx;
}

.toggle {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
  color: #000;
  position: relative;
}

.checked {
  width: auto;
  max-width: 70%;
  margin: 0 10rpx 0 20rpx;
}

.add1 {
  width: 36rpx;
  height: 36rpx;
  margin: 0 20rpx;
}

.mt20 {
  margin-top: 20rpx;
}

.toggle-title {
  width: auto;
  max-width: 350rpx;
}

.mask {
  z-index: 3;
}

.dialog {
  width: 500rpx;
  height: auto;
  border-radius: 30rpx;
  background: #fff;
  position: fixed;
  z-index: 5;
  top: 50%;
  left: 50%;
  overflow: hidden;
  transform: translate(-50%, -50%);
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 28rpx;
}

.dialog-title {
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f2f2f2;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: 500;
}

.dialog-content {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 20rpx;
  position: relative;
}

.dialog-foot {
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  box-sizing: border-box;
  border-top: 2rpx solid #f2f2f2;
}

.dialog-clear {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  right: 40rpx;
  top: 24rpx;
  z-index: 6;
}

.dialog-btn {
  width: 50%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  border-right: 2rpx solid #f2f2f2;
}

.dialog-btn:last-child {
  border: none;
}

.toggle-img {
  width: 40rpx;
  height: 40rpx;
  margin-left: 18rpx;
  position: relative;
  top: 6rpx;
  transition: all 0.5s ease-in;
  transform: rotate(0);
}

.toggle-img1 {
  transition: all 0.5s ease-in;
  transform: rotate(180deg);
}