.container {
  margin: 20rpx auto;
}

.contact {
  margin: 84rpx auto 160rpx !important;
  background-color: #fff;
}

.form {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
}

.lastChild {
  width: 100%;
  height: 100rpx;
  background-color: transparent;
}

.form-item {
  width: 88.73%;
  margin: 0 auto;
  border-bottom: 2rpx solid #F2F2F2;
  box-sizing: border-box;
  height: 110rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
}

.form-item1 {
  height: auto;
  padding: 20rpx 0;
  display: flex;
  flex-direction: column;
}

.form-item-content {
  width: 100%;
  height: auto;
  display: flex;
  line-height: 60rpx;
  justify-content: flex-start;
  align-content: flex-start;
}

.form-item-name {
  width: 160rpx;
  overflow: hidden;
  color: #333;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.form-item-right1 {
  margin-left: 10rpx;
  color: #333;
  justify-content: flex-start !important;
}

.form-item-right-txt1 {
  width: 95%;
  box-sizing: border-box;
  line-height: 42rpx;
  padding-right: 28rpx;
  display: flex;
  justify-content: space-between;
}

.form-item:last-child {
  border: none;
}

.form-item-right {
  display: flex;
  flex: 1;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}

.form-item-right-txt {
  color: #4A90E2;
  margin-right: 28rpx;
}

.t1 {
  color: #BCBCBC;
}

.input1 {
  width: 96%;
  text-align: left;
  font-size: 28rpx;
}

